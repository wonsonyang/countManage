<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.efun.po.TPfBackgroundType" table="t_pf_background_type" >
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="whichTable" type="java.lang.String">
            <column name="whichTable"/>
        </property>
        <property name="platform" type="java.lang.String">
            <column name="platform"/>
        </property>
        <property name="pid" type="java.lang.Long">
            <column name="pid" />
        </property>
        <property name="keyName" type="java.lang.String">
            <column name="keyName" />
        </property>
        <property name="dbValue" type="java.lang.String">
            <column name="dbValue" />
        </property>
        <property name="orderno" type="java.lang.Integer">
            <column name="orderno" />
        </property>
        <property name="remark" type="java.lang.String">
            <column name="remark" />
        </property>
        <property name="createdTime" type="java.util.Date">
            <column name="createdTime" />
        </property>
        <property name="deleted" type="java.lang.String">
            <column name="deleted" />
        </property>
    </class>
</hibernate-mapping>
