<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.efun.po.TPayCashflowConfigTemplate" table="t_pay_cashflow_config_template" >
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="cashflow" type="java.lang.String">
            <column name="cashflow"/>
        </property>
        <property name="configTemplate" type="java.lang.String">
            <column name="configTemplate"/>
        </property>
        <property name="createdTime" type="java.util.Date">
            <column name="createdTime" />
        </property>
        <property name="deleted" type="java.lang.String">
            <column name="deleted" />
        </property>
        <property name="modifiedTime" type="java.util.Date">
            <column name="modifiedTime" />
        </property>
    </class>
</hibernate-mapping>
