<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.efun.po.TPfAreaConfig" table="t_pf_area_config" >
        <id name="id" type="java.lang.String">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="platform" type="java.lang.String">
            <column name="platform">
                <comment>平台标识</comment>
            </column>
        </property>
        <property name="language" type="java.lang.String">
            <column name="language">
                <comment>语言标识</comment>
            </column>
        </property>
        <property name="area" type="java.lang.String">
            <column name="area">
                <comment>地区</comment>
            </column>
        </property>
        <property name="text" type="java.lang.String">
            <column name="text">
                <comment>内容展示</comment>
            </column>
        </property>
        <property name="areaKey" type="java.lang.String">
            <column name="areaKey">
                <comment>地区标识</comment>
            </column>
        </property>
        <property name="areaCode" type="java.lang.String">
            <column name="areaCode">
                <comment>地区区号</comment>
            </column>
        </property>
        <property name="pattern" type="java.lang.String">
            <column name="pattern">
                <comment>正则表达（验证手机号码）</comment>
            </column>
        </property>
        <property name="creator" type="java.lang.String">
            <column name="creator">
                <comment>创建者</comment>
            </column>
        </property>
        <property name="type" type="java.lang.String">
            <column name="type">
                <comment>用途类型区分（登录、储值）</comment>
            </column>
        </property>
        <property name="deleted" type="java.lang.String">
            <column name="deleted">
                <comment>是否删除，0存在1删除</comment>
            </column>
        </property>
        <property name="createdTime" type="java.util.Date">
            <column name="createdTime">
                <comment>创建时间</comment>
            </column>
        </property>
        <property name="modifiedTime" type="java.util.Date">
            <column name="modifiedTime">
                <comment>修改时间</comment>
            </column>
        </property>
        <property name="isHot" type="java.lang.String">
            <column name="isHot">
                <comment>是否热门</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
