		<property name="gameCode" column="gameCode" type="java.lang.String" not-null="false"/>
		<property name="gameName" column="gameName" type="java.lang.String" not-null="false"/>
		<property name="createTime" column="createTime" type="java.util.Date" not-null="true"/>
